<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/popwindow_card_relativelayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/fragment_card_re"
        android:layout_width="match_parent"
        android:layout_height="335dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal">

        <ImageView
            android:id="@+id/fragment_card_image_password"
            android:layout_width="match_parent"
            android:layout_height="330dp"
            android:scaleType="fitXY" />


    </RelativeLayout>

    <LinearLayout
        android:id="@+id/fragment_card_pop_linera_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/fragment_card_re"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/popwindow_card_textview1"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="@drawable/textview_shape"
            android:gravity="center"
            android:text="*"
            android:textSize="0dp" />

        <TextView
            android:id="@+id/popwindow_card_textview2"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="@drawable/textview_shape"
            android:gravity="center"
            android:text="*"
            android:textSize="0dp" />

        <TextView
            android:id="@+id/popwindow_card_textview3"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="@drawable/textview_shape"
            android:gravity="center"
            android:text="*"
            android:textSize="0dp" />

        <TextView
            android:id="@+id/popwindow_card_textview4"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="@drawable/textview_shape"
            android:gravity="center"
            android:text="*"
            android:textSize="0dp" />

        <TextView
            android:id="@+id/popwindow_card_textview5"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="@drawable/textview_shape"
            android:gravity="center"
            android:text="*"
            android:textSize="0dp" />

        <TextView
            android:id="@+id/popwindow_card_textview6"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:background="@drawable/textview_shape"
            android:gravity="center"
            android:text="*"
            android:textSize="0dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/fragment_card_pop_linera_one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/fragment_card_pop_linera_password"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/one"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
 
            android:layout_weight="1" />

        <TextView
            android:id="@+id/two"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
 
            android:layout_toRightOf="@id/one"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/three"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
 
            android:layout_toRightOf="@id/two"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/fragment_card_pop_linera_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_below="@id/fragment_card_pop_linera_one"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/four"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_below="@id/one"
 
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/five"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
 
            android:layout_below="@id/two"
            android:layout_toRightOf="@id/four"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/six"
            android:layout_width="0dp"
            android:layout_height="52dp"
 
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_below="@id/three"
            android:layout_toRightOf="@id/five"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/fragment_card_pop_linera_three"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_below="@id/fragment_card_pop_linera_two"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/seven"
            android:layout_width="0dp"
            android:layout_height="52dp"
 
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/eight"
            android:layout_width="0dp"
 
            android:layout_height="52dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/seven"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/nine"
            android:layout_width="0dp"
            android:layout_height="52dp"
 
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/eight"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/zero_linearlayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_below="@id/fragment_card_pop_linera_three"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/zero"
            android:layout_width="0dp"
 
            android:layout_height="52dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/clean"
            android:layout_width="0dp"
 
            android:layout_height="52dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/close"
            android:layout_width="0dp"
            android:layout_height="52dp"
 
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1" />
    </LinearLayout>


</RelativeLayout>